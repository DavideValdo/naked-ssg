{
  "compilerOptions": {
    "target": "es2016",
    "module": "ES2020",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  }
}
